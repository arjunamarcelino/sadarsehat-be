{
  "name": "sadarsehat-be",
  "version": "0.0.1",
  "description": "Backend for Sadar Sehat",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "prestart": "prisma generate",
    "dev": "tsx watch index.ts",
    "start": "tsx index.ts",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "db:push": "prisma db push",
    "db:fresh": "prisma db push --force-reset",
    "db:reset": "prisma db push --force-reset && tsx ./prisma/seeders/index.ts"
  },
  "author": "Arjuna Marcelino",
  "license": "MIT",
  "imports": {
    "#app/*": "./app/*.js",
    "#prisma/*": "./prisma/*.js"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.2",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.4.1",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "tsx": "^4.19.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prisma": "^6.5.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}
